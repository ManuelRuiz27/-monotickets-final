PWA_NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_BASE}
DASHBOARD_NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_BASE}
R2_ACCESS_KEY_ID=GITHUB_SECRET:R2_ACCESS_KEY_ID
R2_SECRET_ACCESS_KEY=GITHUB_SECRET:R2_SECRET_ACCESS_KEY
R2_BUCKET=GITHUB_SECRET:R2_BUCKET
