# [devops/env-update]
# Staging deployment template. Populate using repository/environment secrets.
APP_ENV=staging
PORT=8080
LOG_FORMAT=json
JWT_SECRET=GITHUB_SECRET:JWT_SECRET
JWT_TTL_HOURS=8
DB_HOST=GITHUB_SECRET:DB_HOST
DB_PORT=5432
DB_USER=GITHUB_SECRET:DB_USER
DB_PASSWORD=GITHUB_SECRET:DB_PASSWORD
DB_NAME=GITHUB_SECRET:DB_NAME
REDIS_URL=GITHUB_SECRET:UPSTASH_REDIS_URL
